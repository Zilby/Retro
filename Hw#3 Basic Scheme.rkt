(define (sq x) (* x x))
;AreaRectangle = AR
(define (AR a b) (* a b))
;sum-of-squares = SOS
(define (SOS a b) (+ (sq a) (sq b)))
;areacircle = AC
(define pi 3.14159)
(define (AC r) (* pi (sq r)))




